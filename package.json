{
    "name": "boilerplate",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "webpack",
        "build:prod": "NODE_ENV=production webpack",
        "build:watch": "webpack-dev-server",
        "test": "jest",
        "test:watch": "yarn test --watch",
        "format": "prettier",
        "lint:js": "eslint",
        "lint:style": "stylelint",
        "generate": "plop",
        "docs": "docz",
        "docs:dev": "yarn docs dev",
        "docs:build": "yarn docs build",
        "dev": "concurrently -c magenta.bold,cyan -n test,build \"yarn test:watch\" \"yarn build:watch\""
    },
    "dependencies": {
        "react": "16.8.4",
        "react-dom": "16.8.4"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "9.x",
        "babel-loader": "^8.0.5",
        "concurrently": "^4.1.0",
        "css-loader": "^2.1.0",
        "docz": "1.0.0-rc.3",
        "docz-theme-default": "^1.0.0-rc.3",
        "eslint": "5.x",
        "eslint-config-react-app": "^3.0.7",
        "eslint-plugin-flowtype": "2.x",
        "eslint-plugin-import": "2.x",
        "eslint-plugin-jsx-a11y": "6.x",
        "eslint-plugin-react": "7.x",
        "husky": "^1.3.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.1.0",
        "jest-dom": "^3.1.2",
        "lint-staged": "^8.1.5",
        "mini-css-extract-plugin": "^0.5.0",
        "plop": "^2.3.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.1.2",
        "prettier": "1.16.4",
        "prop-types": "^15.7.2",
        "purgecss-webpack-plugin": "^1.4.0",
        "react-testing-library": "^6.0.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.10.1",
        "stylelint-config-recommended": "^2.1.0",
        "tailwindcss": "^0.7.4",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1"
    }
}
